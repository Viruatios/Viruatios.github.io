---
import BaseLayout from "../layouts/BaseLayout.astro";

import Greeting from "../components/Greeting";

const pageTitle = "CuLoo's Homepage";
---

<BaseLayout pageTitle={pageTitle}>
	<h2>我超棒的博客的副标题！</h2>
	<p>学习Astro入门教程构建的版本。</p>

	<Greeting
		client:load
		messages={[
			"Hej",
			"Hallo",
			"Hola",
			"Habari",
			"Hi",
			"Hello",
			"Howdy",
			"Hey there",
		]}
	/>
</BaseLayout>
